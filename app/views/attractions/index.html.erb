<% @attractions.each do |attraction| %>
  <% if @user.admin %>
  <% #binding.pry %>
    <%= link_to(attraction_path(attraction)) do %>
      Show <%= attraction.name %><br>
    <% end %>
  <% else %>
    <%= link_to(attraction_path(attraction)) do %>
      Go on <%= attraction.name %><br>
    <% end %>
  <% end %>
<% end %>

<% if @user.admin %>
  <%= link_to 'New Attraction',  new_attraction_path%>
<% end %>
